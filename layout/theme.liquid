<!--[if IE 7]><html class="no-js ie7 oldie" lang="en"><![endif]-->
<!--[if IE 8]><html class="no-js ie8 oldie" lang="en"><![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" lang="en"><!--<![endif]-->
<head>
<title>{{ page_title }}</title>
<meta name="description" content="{{ page_description }}" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link rel="canonical" href="{{ canonical_url }}" />
<link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />
{% include 'fb-open-graph-tags' %}
{% include 'twitter-card' %}
{{ 'main.min.css' | asset_url | stylesheet_tag }}
{{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js' | script_tag }}
{{ 'option_selection.js' | asset_url | script_tag }}
{{ 'api.jquery.js' | shopify_asset_url | script_tag }}
{% if template contains 'customers' %}
{{ 'shopify_common.js' | shopify_asset_url | script_tag }}
{{ 'customer_area.js'  | shopify_asset_url | script_tag }}
{% endif %}
{% capture CFH %}{{ content_for_header  }}{% endcapture %}{{ CFH }}
<link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}" type="image/x-icon" />
</head>

<body class="gridlock {{ page.handle }}  {{ template }} ">
  <div id="wrapper" class="shifter-page">
    {% include 'header' %}
    {% if template != 'index' %}
    {% include 'page-header' %}
    {% include 'toolbar' %}
    {% endif %}

    <section id="content">
      <div>
        {% if template == 'list-collections' %}
        {% include 'collection-listing' %}
        {% else %}
        {{ content_for_layout }}
        {% endif %}
      </div>
    </section><!-- Content -->

    <footer id="store-footer">
    <div class="row">
      <hr class="desktop-12">
      <div class="desktop-6">
      <p>
        {{ settings.right_content }}

<div class="social-icons">
{% for i in (1..6) %}
{% capture show_social %}show_social_item_{{ i }}{% endcapture %}
{% capture icon %}show_social_image_{{ i }}{% endcapture %}
{% capture link %}social_item_{{ i }}_link{% endcapture %}
{% capture alt %}homeslide_item_{{ i }}_alt{% endcapture %}
{% capture sub %}homeslide_item_{{ i }}_sub{% endcapture %}
{% if settings[show_social] %}
<a target="blank" href="{{ settings[link] }}"><i class="ss-social-circle ss-icon">{{ settings[icon] }}</i></a>
{% endif %}
{% endfor %}
</div>

</p>
</div>
<div class="desktop-6 text-right">
<p>
{{ settings.left_content }}
</p>
</div>
</div>
</footer>
{% include 'footer' %}
</div><!-- Wrapper -->

<div id="search-footer" class="gridlock" style="display:none;">
<a href="#" class="close"><i class="ss-icon ss-gizmo">close</i></a>
<div class="table">
<div class="cell">
<div class="row">
<div class="desktop-12">
<form class="search" action="/search">
<fieldset>
<label for="search">Search:</label>
<input type="text" name="q" placeholder="Tote bag, mug, etc." id="search" class="search_box"  value="" />
</fieldset>
</form>
</div>
</div>
</div>
</div>
</div>

<!--End mc_embed_signup-->
